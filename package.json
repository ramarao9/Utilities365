{
  "name": "utilities365",
  "description": "Tool to help you easily perform certain Dynamics365 tasks in a few simple steps",
  "productName": "Utilities365",
  "version": "1.0.0",
  "author": "Koneru, Rama",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@azure/msal-node": "^1.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "axios": "^0.24.0",
    "bulma": "^0.9.3",
    "bulma-checkradio": "^1.1.1",
    "bulma-switch": "^2.0.0",
    "dynamics-web-api": "^1.7.4",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.0.1",
    "is-empty": "^1.2.0",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-json-tree": "^0.15.1",
    "react-redux": "^7.2.6",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "redux": "^4.1.2",
    "url": "^0.11.0"
  },
  "main": "src/electron-starter.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\" \"npm run css-watch\"",
    "electron-pack": "build â€” em.main=build/electron-starter.js",
    "preelectron-pack": "npm build",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "css-build": "node-sass --omit-source-map-url src/sass/appstyles.scss src/css/appstyles.css",
    "css-watch": "npm run css-build --watch"
  },
  "devDependencies": {
    "@types/is-empty": "^1.2.1",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-json-tree": "^0.13.0",
    "@types/react-redux": "^7.1.20",
    "@types/react-router-dom": "^5.3.2",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "electron": "^16.2.6",
    "electron-builder": "^22.14.5",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "node-sass": "^7.0.0",
    "react-scripts": "^4.0.3",
    "standard": "^16.0.4",
    "typescript": "^4.5.3",
    "typescript-tslint-plugin": "^1.0.1",
    "wait-on": "^6.0.0"
  },
  "build": {
    "appId": "com.electron.utilities365",
    "productName": "Utilities365",
    "mac": {
      "target": "dmg",
      "icon": "build/u365.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "build/u365.ico"
    },
    "directories": {
      "buildResources": "assets"
    },
    "extends": null
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
