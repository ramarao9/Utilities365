{
  "name": "utilities365",
  "description": "Tool to help you easily perform certain Dynamics365 tasks in a few simple steps",
  "productName": "Utilities365",
  "version": "1.0.0",
  "author": "Koneru, Rama",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-regular-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "adal-node": "^0.2.1",
    "axios": "^0.19.0",
    "bulma": "^0.8.0",
    "bulma-switch": "^2.0.0",
    "dynamics-web-api": "^1.5.14",
    "electron-is-dev": "^1.1.0",
    "electron-store": "^5.1.0",
    "is-empty": "^1.2.0",
    "path": "^0.12.7",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-json-tree": "^0.11.2",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "url": "^0.11.0"
  },
  "main": "src/electron-starter.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\" \"npm run css-watch\"",
    "electron-pack": "build â€” em.main=build/electron-starter.js",
    "preelectron-pack": "npm build",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "css-build": "node-sass --omit-source-map-url src/sass/appstyles.scss src/css/appstyles.css",
    "css-watch": "npm run css-build --watch"
  },
  "devDependencies": {
    "@types/is-empty": "^1.2.0",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.12",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "@types/react-json-tree": "^0.6.11",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.2",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "electron": "^7.1.2",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "node-sass": "^4.13.0",
    "react-scripts": "^3.2.0",
    "standard": "^14.3.1",
    "typescript": "^3.7.2",
    "typescript-tslint-plugin": "^0.5.5",
    "wait-on": "^3.3.0"
  },
  "build": {
    "appId": "com.electron.utilities365",
    "productName": "Utilities365",
    "directories": {
      "buildResources": "assets"
    },
    "extends": null
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
